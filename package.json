{
  "name": "multichainstorage",
  "version": "1.0.0",
  "description": "Multi-Chain Storage",
  "main": "main.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config buildVue/webpack.dev.conf.js",
    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue src",
    "build:test": "cross-env NODE_ENV=test env_config=test node buildVue/build.js",
    "build:test_prod": "cross-env NODE_ENV=test env_config=test_prod node buildVue/build.js",
    "build:calibration": "cross-env NODE_ENV=production env_config=calibration node buildVue/build.js",
    "build:prod": "cross-env NODE_ENV=production env_config=prod node buildVue/build.js",
    "build:semi_prod": "cross-env NODE_ENV=production env_config=semi_prod node buildVue/build.js",
    "dev_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run dev\n",
    "build:test_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:test\n",
    "build:test_prod_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:test_prod\n",
    "build:cali_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:calibration\n",
    "build:prod_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:prod\n",
    "build:semi_prod_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:semi_prod\n",
    "build:test_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run build:test\n",
    "build:prod_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run build:prod\n",
    "build:cali_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run build:calibration\n",
    "electron:start": "electron .",
    "electron:dist": "npm run build:prod && electron-builder",
    "electron:dist_win": "npm run build:prod_t && electron-builder --win  --x64",
    "electron:dist_darwin": "npm run build:prod_u && electron-builder --darwin  --x64",
    "electron:packager": "electron-packager . release --platform=darwin --arch=x64 --icon=build/MCS_logo.png --out=./dist --asar --app-version=1.0.0  --overwrite --ignore=\"(release|dist|src|docs|.gitignore|LICENSE|README.md|webpack.config*|node_modules)\""
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "echarts": "^5.4.0",
    "element-resize-detector": "^1.2.3",
    "element-ui": "^2.15.6",
    "jquery": "^3.6.0",
    "js-md5": "^0.7.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "spark-md5": "^3.0.2",
    "swiper": "^5.2.0",
    "vue": "^2.5.2",
    "vue-drag-verify": "^1.0.6",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.0.1",
    "vue-simple-uploader": "^0.7.6",
    "vuex": "^3.6.2",
    "web3": "^1.7.0"
  },
  "devDependencies": {
    "@metamask/sdk": "^0.1.0",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.0.1",
    "compression-webpack-plugin": "^1.1.12",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^3.2.0",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "electron-package": "^0.1.0",
    "electron-updater": "^5.3.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-notifier": "^5.1.2",
    "node-sass": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^10.4.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.0.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^2.1.0",
    "vue-i18n": "^8.25.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
